<div class="page-header">
  <h3>Pantry #<%= @pantry.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @pantry.user_id %></dd>

      <dt>Food</dt>
      <dd><%= @pantry.food_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/pantries" class="btn btn-primary">
        Back
      </a>
      <a href="/pantries/<%= @pantry.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_pantry/<%= @pantry.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
